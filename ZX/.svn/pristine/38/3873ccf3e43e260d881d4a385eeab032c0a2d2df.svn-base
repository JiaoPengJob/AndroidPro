<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/search_bgm"
    android:orientation="vertical">

    <android.support.v4.app.FragmentTabHost
        android:id="@+id/fth_host_cooperation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"></android.support.v4.app.FragmentTabHost>

    <FrameLayout
        android:id="@+id/fragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="0"></FrameLayout>

    <com.tch.zx.view.CustomViewPager
        android:id="@+id/cvp_view_pager_cooperation"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"></com.tch.zx.view.CustomViewPager>



</LinearLayout>