<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    tools:context="com.tch.youmuwa.ui.activity.worker.WorkerMainActivity">

    <com.ashokvarma.bottomnavigation.BottomNavigationBar
        android:id="@+id/bottomWorkerMenu"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"></com.ashokvarma.bottomnavigation.BottomNavigationBar>

    <FrameLayout
        android:id="@+id/flWorkerFragmentShow"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@id/bottomWorkerMenu"></FrameLayout>

</RelativeLayout>
